{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Run Extension",
			"type": "extensionHost",
			"request": "launch",
			"args": ["--extensionDevelopmentPath=${workspaceFolder}"],
			"sourceMaps": true,
			"outFiles": ["${workspaceFolder}/extension.js", "${workspaceFolder}/extension.js.map"],
			"preLaunchTask": "${defaultBuildTask}"
		}
	]
}
